
  <div class = "card-body">
    <div class = "card report-details" >
      <div class="accordion md-accordion" id="accordionEx" role="tablist" >
        <div class="card-header" id= "card-header"role="tab">
            Wage Report Overview
        </div>
          <div class = 'row'>
            <div class = 'col'>
              <p class = "data-header">Begin Date</p>
              <p ><%=@current_report.timespan.begin_on%></p>
            </div>
            <div class = 'col'>
              <p class = "data-header">End Date</p>
              <p><%=@current_report.timespan.end_on%></p>
            </div>
            <div class = 'col'>
              <p class = "data-header">Submitted At</p>
              <p ><%=@current_report.submitted_at.to_date%></p>
            </div>
            <div class = 'col'>
              <p class="data-header">Status</p>
            <p ><%=@current_report.status.capitalize%></p>
              </div>
            <div class = 'col'>
                <p class="data-header">Submission Kind</p>
                <p ><%=@current_report.submission_kind.capitalize%></p>         
            </div>
           </div>
          <div class = 'row'>
            <div class = 'col'>
              <p  class="data-header">Total State Gross Wages</p>
                <p ><%=number_to_currency(@current_report.sum_state_total_wages)%></p>
            </div>
            <div class = 'col'>
              <p class="data-header">Total State UI Wages</p>
              <p><%=number_to_currency(@current_report.sum_state_ui_total_wages)%></p>
             </div>
            <div class ='col'>
              <p class="data-header"> State UI Taxable Wages</p>
              <p><%=number_to_currency(@current_report.sum_state_taxable_wages)%></p>
              </div>
            <div class = 'col'>
              <p class="data-header"> State UI Excess Wages</p>
              <p><%=number_to_currency(@current_report.sum_state_excess_wages)%></p>
            </div>
            <div class = 'col'>
              <p class="data-header"> Total Employees</p>
              <p><%=(@current_report.sum_employees) -1 %></p>
            </div>
      </div>

</div>
