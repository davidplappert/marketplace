<% if @organization.wage_reports.present?%>
<div class="card-body">
  <div class="card">
    <li class="list-group-item"id="card-header">Latest Report -  <%= "#{@latest_timespan.title}"%> </li>
    <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
      <thead>
        <tr>
          <td class="td-sm">Submitted At
          </td>
          <td class="td-sm">Submission Kind
          </td>
          <td class="td-sm">Total Wages
          </td>
          <td class="td-sm">Taxable Wages
          </td>
          <td class="td-sm">Excess Wages
          </td>
          <td class="td-sm"> Administrative Assessment
          </td>        
            <td class="td-sm">Interest
          </td>       
             <td class="td-sm">Penalty
          </td>
          <td class="td-sm">Actions
          </td>
        </tr>
      </thead>
      <tbody>
        <% @current_reports.each do |current_report|%>
        <tr>
          <td><%= link_to "#{current_report.submitted_at.to_date}", wage_report_path(current_report), :class => "card-header"%></td>
          <td><%=current_report.submission_kind.capitalize%></td>
          <td><%=number_to_currency(current_report.sum_state_total_wages)%></td>
          <td><%=number_to_currency(current_report.sum_state_taxable_wages)%></td>
          <td><%=number_to_currency(current_report.sum_state_excess_wages)%></td>
          <td><%=number_to_currency(0)%></td>    
          <td><%=number_to_currency(0)%></td>
          <td><%=number_to_currency(0)%></td>
          <td> <%=link_to 'Amend', edit_wage_report_path(current_report), :class => "btn btn-outline-primary btn-rounded ", :id => "plus"  %></td>
        </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>

<%else %>


<div class="card-body">
  <div class="card">
    <li class="list-group-item"id="card-header">Latest Reports </li>
    <div class = "no-reports">
    No Current Reports
    </div>

  </div>
</div>


<%end%>
