
	<div class="col-md-4 mt-4">
		<% @current_reports.each do |current_report|%>
		<div class="card">
			<div class="card-header text-center">Latest Report Versions - <%= "#{@all_quarters.first.title}"%><br />
			</div>
				<div class="card-body p-0">
					<p class="card-title text-center mb-1 mt-2"><strong>Total Wages</strong></p>
					<h4 class="text-center"><%=number_to_currency(current_report.state_total_gross_wages)%></h4>
					<hr>
					<p class="card-title text-center mb-1 mt-2"><strong>Excess Wages</strong></p>
					<h4 class="text-center text-success"><%=number_to_currency(current_report.state_ui_taxable_wages)%></h4>
					<hr>
					<p class="card-title text-center mb-1 mt-2"><strong>UI Amount Due</strong></p>
					<h4 class="text-center text-danger"><%=number_to_currency(current_report.ui_total_due)%></h4>
					<hr>
					<%=link_to 'Amend', edit_wage_report_path(current_report), class:'btn btn-raised btn-success btn-block btn-lg col-11 ml-3' %>
					<p class="card-text mb-0 pl-2"><small class="text-muted">Status - <%= current_report.submission_kind.capitalize %></small></p>
					<p class="card-text mb-0 pl-2"><small class="text-muted">Submission Kind - <%= current_report.submission_kind.capitalize %></small></p>
					<% end %>
				</div>
  		</div>
		</div>
