<div class="card-body">
  <div class="card">
    <li class="list-group-item"id="card-header">Latest Report Versions -  <%= "#{@all_quarters.first.title}"%> </li>
    <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
      <thead>
        <tr>
          <td class="td-sm">Status
          </td>
          <td class="td-sm">Submission Kind
          </td>
          <td class="td-sm">Total Wages
          </td>
          <td class="td-sm">Excess Wages
          </td>
          <td class="td-sm">UI Amount Due
          </td>
          <td class="td-sm">Actions
          </td>
        </tr>
      </thead>
      <tbody>
        <% @current_reports.each do |current_report|%>
        <tr>
          <td><%= link_to "#{current_report.status.capitalize}", wage_report_path(current_report), :class => "card-header"%></td>
          <td><%=current_report.submission_kind.capitalize%></td>
          <td><%=number_to_currency(current_report.state_total_gross_wages)%></td>
          <td><%=number_to_currency(current_report.state_ui_taxable_wages)%></td>
          <td><%=number_to_currency(current_report.ui_total_due)%></td>
          <td> <%=link_to 'Amend', edit_wage_report_path(current_report), :class => "btn btn-outline-primary btn-rounded ", :id => "plus"  %></td>
        </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>