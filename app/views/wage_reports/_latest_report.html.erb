<div class="card-body">
  <div class="card">
    <li class="list-group-item active">Latest Report Versions -  <%= "#{@all_quarters.first.title}"%> </li>
    <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
      <thead>
        <tr>
          <td class="td-sm">Status
          </td>
          <td class="td-sm">Submission Kind
          </td>
          <td class="td-sm">Total Wages
          </td>
          <td class="td-sm">Excess Wages
          </td>
          <td class="td-sm">UI Amount Due
          </td>
          <td class="td-sm">Ammend
          </td>
        </tr>
      </thead>
      <tbody>
        <% @organization.wage_reports.filter_by_date_and_status(@all_quarters.first).each do |report|%>
        <tr>
          <td><%= link_to "#{report.status.capitalize}", wage_report_path(report)%></td>
          <td><%=report.submission_kind.capitalize%></td>
          <td>$<%=report.state_total_gross_wages%></td>
          <td>$<%=report.state_ui_taxable_wages%></td>
          <td>$<%=report.ui_total_due%></td>
          <td> <%=link_to '+', edit_wage_report_path(report), :class => "btn btn-outline-primary btn-rounded ", :id => "plus"  %></td>
        </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>