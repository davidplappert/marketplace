
    <table  class="table table-striped  " id="new-entry">
      <thead>
        <tr>
          <td class="">Employee</td>
          <td class="td-sm">Submission Kind</td>
          <td class="td-sm">Submitted At</td>
          <td class="td-sm">UI Wages Paid</td>
          <td class="td-sm">Amend Reason</td>

      </tr>
      </thead>
      <tbody>
        <% @entries.each do |entry| %>
          <tr>
            <td><%= entry.try(:wage).try(:person_party).try(:current_first_name) + " " + entry.try(:wage).try(:person_party).try(:current_last_name)%></td>
            <td scope="row"><%= entry.submission_kind.capitalize%></td>
            <td scope="row"><%= entry.submitted_at.to_date%></td>
            <td>$ <%= entry.try(:wage).try(:state_total_gross_wages)%></td>
            <td> <%= entry.amend_reason.capitalize%></td>
          </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>